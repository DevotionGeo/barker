<div class="row">
  <div id="messages-list" class="col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3">
    <h3><%= pluralize( @users.size, 'result' ) %></h3>
    <ul class="list-group search-list">
      <% @users.each do |user| %>
        <li class="list-group-item">
          <% display_name = user.first_name + " " + user.last_name + " aka " + user.profile_name %>
          <span><%= link_to display_name, messages_path(profile_id: user) %></span>
        </li>
      <% end %>
    </ul>
  </div>
</div>
